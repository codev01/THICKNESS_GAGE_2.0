<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
	</startup>
	<appSettings>
		<!--Перезапуск приложения при возникновении ошибки-->
		<add key="RestartTime_Seconds" value="10"/>
		
		<!--Длина (байт) телеграммы по протоколу-->
		<add key="Protocol_Telegram_Length" value="80"/>
		<!--Длина (байт) COILID по протоколу-->
		<add key="CoilID_MSG_Length" value="10"/>
		<!--Смещение (байт) COILID по протоколу-->
		<add key="CoilID_MSG_Offset" value="8"/>

		<!--Отправляем сообщения?-->
		<add key="IsSendingMessages" value="True"/>
		<!--IP адресс устройства с которым обмениваемся сообщениями-->
		<add key="SocketHost" value="10.96.163.101"/>
		<!--Порт подключения к устройству-->
		<add key="SocketPort" value="8001"/>
		
		<add key="OracleUser" value="gauge_usr"/>
		<add key="OraclePassword" value="gauge_pwd"/>
		<add key="OraclePackage" value="pck_gauge"/>
		<add key="OracleWriteToDataBase" value="True"/>
		<add key="OracleDataSource" value="(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=localhost)(PORT=1521))(CONNECT_DATA=(SERVER=DEDICATED)(SERVICE_NAME=CGL_S)))"/>
	</appSettings>
</configuration>
